<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="fleet_cancel_action" model="ir.actions.act_window">
    <field name="name">Cancel reason</field>
    <field name="res_model">fleet.cancel.reason</field>
    <field name="view_mode">tree,form</field>
    </record>

    <record id="fleet_cancel_reason_view_tree" model="ir.ui.view">
        <field name="name">fleet.cancel.reason.tree</field>
        <field name="model">fleet.cancel.reason</field>
        <field name="arch" type="xml">
            <tree string="Channel" editable="bottom">
                <field name="name"/>
            </tree>
        </field>
    </record>
</odoo>